    },
    {
        "text": "Put your right index finger on the scanner.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_11_col0.mp3"
    },
    {
        "text": "Put your right index finger on the scanner.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_11_col3.mp3"
    },
    {
        "text": "Please remain standing and raise your right hand.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_12_col0.mp3"
    },
    {
        "text": "Please remain standing and raise your right hand.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_12_col3.mp3"
    },
    {
        "text": "Please read the sentence on the tablet.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_13_col0.mp3"
    },
    {
        "text": "Please read the sentence on the tablet.",
