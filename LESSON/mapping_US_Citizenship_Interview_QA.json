    },
    {
        "text": "Please write the following sentence.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_14_col0.mp3"
    },
    {
        "text": "Please write the following sentence.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_14_col3.mp3"
    },
    {
        "text": "Please tell me your Social Security Number.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_15_col0.mp3"
    },
    {
        "text": "Please tell me your Social Security Number.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_15_col3.mp3"
    },
    {
        "text": "Please spell your last name.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_16_col0.mp3"
    },
    {
        "text": "Please spell your last name.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_16_col3.mp3"
    },
    {
        "text": "Please write down the new name.",
        "file": "audio_US_Citizenship_Interview_QA/audio_US_Citizenship_Interview_QA_17_col0.mp3"
    },
    {
        "text": "Please write down the new name.",
